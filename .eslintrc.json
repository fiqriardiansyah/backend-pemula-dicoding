{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "import/no-extraneous-dependencies": "off",
        "no-console": "off",
        "linebreak-style": "off",
        "import/extensions": "off"
    },
    "plugins": ["prettier"]
}
